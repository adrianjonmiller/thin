{
  "name": "thin.css",
  "version": "1.0.22",
  "description": "Simple, css framework for apps.",
  "main": "dist/thin.css",
  "unpkg": "dist/thin.css",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build1.0": "node-sass --include-path scss src/default/index.scss dist/thin.css && npm run autoprefix",
    "build2.0": "sass src/2.0/index.scss dist/thin.css && npm run autoprefix",
    "autoprefix": "postcss dist/thin.css --use autoprefixer -b 'last 2 versions' -d dist",
  },
  "nodemonConfig": {
    "ignore": [
      "src/app/**/*.js",
      "public/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrianjonmiller/thin.git"
  },
  "keywords": [
    "sass",
    "scss",
    "css",
    "cdn"
  ],
  "author": "Adrian J Miller",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adrianjonmiller/thin/issues"
  },
  "homepage": "https://thin.style",
  "dependencies": {
    "fibers": "5.0.0",
    "sass": "1.32.8"
  }
}
